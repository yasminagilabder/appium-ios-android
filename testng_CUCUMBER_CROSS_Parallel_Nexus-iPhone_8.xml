<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="CROSS_Parallel_Nexus-iPhone_8" parallel="tests" thread-count="2">
    <test name="Nexus">
        <parameter name="deviceName" value="Nexus S API 22"/>
        <parameter name="UDID" value="emulator-5554"/>
        <parameter name="platformVersion" value="5.1.0"/>
        <parameter name="node_port" value="1234"/>
        <classes>
            <class name="acceptanceTests.runners.AndroidCucumberRunnerTest">
            </class>
        </classes>
    </test>
    <test name="iPhone 8">
        <parameter name="wda" value="8200"/>
        <parameter name="node_port" value="4790"/>
        <parameter name="deviceName" value="iPhone 8"/>
        <parameter name="platformVersion" value="13.5"/>
        <classes>
            <class name="acceptanceTests.runners.IOSCucumberRunnerTest">
            </class>
        </classes>
    </test>
</suite>

