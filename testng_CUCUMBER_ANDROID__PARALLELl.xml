<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="CUCUMBER-ANDROID_Parallel" parallel="tests" thread-count="2">

    <test name="NexusTest">
        <parameter name="deviceName" value="Nexus S API 22"/>
        <parameter name="UDID" value="emulator-5554"/>
        <parameter name="platformVersion" value="5.1.0"/>
        <parameter name="node_port" value="1234"/>
        <classes>
            <class name="acceptanceTests.runners.AndroidCucumberRunnerTest">
            </class>
        </classes>
    </test>
    <test name="PixelTest">
        <parameter name="deviceName" value="Pixel 3 XL API 29"/>
        <parameter name="UDID" value="emulator-5556"/>
        <parameter name="platformVersion" value="10.0.0"/>
        <parameter name="node_port" value="3456"/>
        <classes>
            <class name="acceptanceTests.runners.AndroidCucumberRunnerTest">
            </class>
        </classes>
    </test>

</suite>

